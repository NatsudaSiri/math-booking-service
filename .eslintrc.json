{
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "indent": ["error", 2],
    "prefer-const": 1,
    "import/prefer-default-export": 0,
    "semi": 1,
    "no-unused-expressions": 0,
    "max-len": 0,
    "no-restricted-syntax": ["off", "ForOfStatement"],
    "arrow-parens": "off",
    "function-paren-newline": "off",
    "object-curly-newline": 0,
    "no-underscore-dangle": "off"
  }
}